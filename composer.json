{
    "name": "daxslab/enzona-sdk",
    "version": "1.0.0",
    "description": "",
    "keywords": [
        "enzona",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "http://github.com/daxslab/enzona-sdk-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Daxslab and contributors",
            "homepage": "http://github.com/daxslab/enzona-sdk-php"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2",
        "daxslab/enzona-qr": "*@dev",
        "daxslab/enzona-payment": "*@dev"
    },
    "require-dev": {
